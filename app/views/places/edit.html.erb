<%= form_with model: @place, local: true do |f| %>
  <%= f.text_area :name, placeholder:"練習場所の名前（必須 40文字まで)", maxlength:"40" %>
  <%= f.text_area :address, placeholder:"練習場所の住所（必須 40文字まで)", maxlength:"40" %>
  <%= f.text_area :remark, placeholder:"練習場所の住所（必須 40文字まで)", maxlength:"40" %>
  <%= f.text_field :price, placeholder:"例）300"  %><span>円</span>
  <%= f.submit "新しい練習場所を登録する！" %>
  <%=link_to 'もどる', root_path%>
<% end %>