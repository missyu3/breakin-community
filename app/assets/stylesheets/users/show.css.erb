  border-radius:50%;
  margin: 0 auto;
  position:absolute;
  top: 5%;
  left: 5% ;
  background-color: rgba(0,0,0,0.3);
}

.user-profile {
  height:70%;
  display: flex;
  justify-content: center;
  flex-direction:column;
  align-items: center;
  color:#dedede;
  font-size:20px;
  font-weight:bold;
  padding:5px 0;
}
.user-relationship, .user-crew, .user-about, .user-nickname, .user-edit, .user-dance-level {
  height:15%;
}

.user-dance-level{
  font-size: 10px;
}

.user-remark {
  font-size: 10px;
  height:40%;
}

.user-relationship > a, .user-crew > a,.user-edit > a,.user-about > a, .user-nickname > a{
  font-size: 10px;
  text-decoration:none;
  color:#dedede;
  padding:10px;
  position: relative;
  width: 256px;
  transition: all 0.3s;
}

.user-relationship > a::before, .user-relationship > a::after, .user-crew > a::before, .user-crew > a::after , .user-edit > a::before, .user-edit > a::after, .user-about > a::before, .user-about > a::after {
  content: '';
  position: absolute;
  transition: all 0.3s;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}

.user-relationship > a::before, .user-crew > a::before, .user-edit > a::before , .user-about > a::before {
    z-index: 1;
  
  opacity: 0;
  background: rgba(51, 112, 121, 0.5);
  transform: scale(0.1, 1);
}

.user-relationship > a:hover::before, .user-crew > a:hover::before, .user-edit > a:hover::before, .user-about > a:hover::before {
  opacity: 1;
  transform: scale(1, 1);
}

.user-relationship > a::after, .user-crew > a::after, .user-edit > a::after, .user-about > a::after {
  transition: all 0.3s;
  border: 1px solid rgba(51, 112, 121, 0.5);
}

.user-relationship > a:hover::after, .user-crew > a:hover::after, .user-edit > a:hover::after, .user-about > a:hover::after {
  transform: scale(1, .1);
  opacity: 0;
}

